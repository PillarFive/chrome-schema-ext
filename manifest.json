{
  "manifest_version": 3,
  "name": "HTML Schema Generator",
  "version": "0.0.0.1",
  "description": "Parses a page's HTML and returns an HTML schema.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["contentScript.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["iframeScript.js", "iframeStyles.css"],
      "matches": ["*://*/*"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"  
    }
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}

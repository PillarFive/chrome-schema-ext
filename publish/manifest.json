{
  "manifest_version": 3,
  "name": "HTML Schema Generator",
  "version": "0.0.0.1",
  "description": "Parses a page's HTML and returns an HTML schema.",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["activeTab", "scripting", "sidePanel"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["utils.js", "treeBuilder.js", "schemaGenerator.js"]
    }
  ],
  "action": {
    "default_title": "Open HTML Schema"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}
